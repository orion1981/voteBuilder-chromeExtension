{
  "manifest_version": 2,
  "name": "VoteBuilder Chrome Extension",
  "version": "1.0",
  "description": "Adding NOTES to a space that is more relevant for use in VoteBuilder",
  "content_scripts":[
    {
      "matches": [
        "<all_urls>",
        "<https://www.votebuilder.com/ContactDetailScript.aspx?FromSortPage=1&CurrentVirtualPhoneBank=1>",
      ],
      "js": ["myscript.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "VoteBuilder.png"
  }
}
