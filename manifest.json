{
  "manifest_version": 2,
  "name": "VoteBuilder Chrome Extension",
  "version": "1.0",
  "description": "Adding NOTES to a space that is more relevant for use in VoteBuilder",
  "content_scripts":[
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [ "jquery.3.3.1.min.js", "myscript.js"]
    }
  ],
  "background": {
    "scripts": ["jquery.3.3.1.min.js", "background.js"]
  },
  "browser_action": {
    "default_icon": "VoteBuilder.png"
  }
}
